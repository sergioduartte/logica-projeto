-- main: AppStore FSM
MODULE main
VAR
  aplicativos : process aplicativoFSM;
  contas : process contaFSM;

MODULE contaFSM
VAR
  containsCreditoSuficiente : {True, False};
  aplicativos : process aplicativoFSM;
  dispositivos : process dispositivoFSM;
ASSIGN
  init(containsCreditoSuficiente) := True;

MODULE aplicativoFSM
VAR
  versao : {v0, v1, v2};
  isGratuito : {True, False};
ASSIGN

MODULE dispositivoFSM
VAR
  appsVersoesInstaladas : process aplicativoFSM;
ASSIGN
